] = useStoreContext();\n  const { id } = useParams();\n\n  const [currentProduct, setCurrentProduct] = useState({});\n\n  const { loading, data } = useQuery(QUERY_PRODUCTS);\n\n  const { products } = state;\n\n  useEffect(() => {\n    if (products.length) {\n      setCurrentProduct(products.find((product) => product._id === id));\n    } else if (data) {\n      dispatch({\n        type: UPDATE_PRODUCTS,\n        products: data.products,\n      });\n    }\n  }, [products, data, dispatch, id]);\n\n  const addToCart = () => {\n    dispatch({\n      type: ADD_TO_CART,\n      product: { ...currentProduct, purchaseQuantity: 1 }\n    });\n  }\n\n  return (\n    <>\n      {currentProduct ? (\n        <div className=\"container my-1\">\n          <Link to=\"/\">‚Üê Back to Products</Link>\n\n          <h2>{currentProduct.name}</h2>\n\n          <p>{currentProduct.description}</p>\n\n          <p>\n            <strong>Price:</strong>${currentProduct.price}{' '}\n            <button>Add to Cart</button>\n            <button>Remove from Cart</button>\n          </p>\n\n          <img\n            src={`/images/${currentProduct.image}`}\n            alt={currentProduct.name}\n          />\n        </div>\n      ) : null}\n      {loading ? <img src={spinner} alt=\"loading\" /> : null}\n      <Cart />\n    </>\n  );\n}\n\nexport default Detail;\n","/Users/francellaaguilar/Desktop/Shop-Shop/client/src/pages/Home.js",[],"/Users/francellaaguilar/Desktop/Shop-Shop/client/src/pages/Login.js",[],"/Users/francellaaguilar/Desktop/Shop-Shop/client/src/pages/NoMatch.js",[],"/Users/francellaaguilar/Desktop/Shop-Shop/client/src/pages/OrderHistory.js",[],"/Users/francellaaguilar/Desktop/Shop-Shop/client/src/utils/GlobalState.js",[],"/Users/francellaaguilar/Desktop/Shop-Shop/client/src/components/Nav/index.js",[],"/Users/francellaaguilar/Desktop/Shop-Shop/client/src/utils/auth.js",[],"/Users/francellaaguilar/Desktop/Shop-Shop/client/src/utils/actions.js",[],"/Users/francellaaguilar/Desktop/Shop-